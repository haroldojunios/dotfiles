Host *
  UpdateHostKeys yes

Host celular
  Port 8022

Match host celular exec "nc -w 1 -z 192.168.1.212 %p"
  HostName 192.168.1.212
Match host celular exec "nc -w 1 -z 192.168.6.222 %p"
  HostName 192.168.6.222
Match host celular exec "nc -w 1 -z 10.0.0.12 %p"
  HostName 10.0.0.12
Match host celular exec "nc -w 1 -z 192.168.101.12 %p"
  HostName 192.168.101.12
Match host celular exec "true"
  HostName 192.168.1.22

Host rasp
  HostName rasp.local
  User pi

Host rasp-ubuntu
  HostName rasp-ubuntu.local
  User haroldo

Host pc
  HostName 192.168.1.15
  User haroldo

Host raspap
  HostName 10.3.141.1
  User pi

Host laserscanner1.local
  User rural
